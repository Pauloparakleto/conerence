<h1>Tracks</h1>
<p>Tracks has many talks</p>

<section>
  <h2>
    <%= link_to "Create", new_track_path %>
  </h2>
  <table>
    <tr>
      <th>Name</th>
    </tr>
    <% @tracks.each do |track| %>
      <tr>
        <td><%= link_to track.name, track_path(track) %></td>
      </tr>
    <% end %>
  </table>
</section>

